{
  "name": "kp-website",
  "version": "0.0.1",
  "author": "thymikee",
  "description": "kp-website",
  "scripts": {
    "build": "gulp build"
  },
  "chisel": {
    "src": {
      "assets": "src/assets/**/*",
      "app": "src/scripts/app.js",
      "base": "src",
      "scripts": "src/scripts/**/*",
      "styles": "src/styles/**/*",
      "stylesMain": "src/styles/*.scss",
      "templates": "src/templates/**/*.{twig,html}"
    },
    "dest": {
      "assets": "dist/assets",
      "base": "dist",
      "revManifest": "dist/rev-manifest.json",
      "scripts": "dist/scripts",
      "styles": "dist/styles"
    }
  },
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel-preset-es2015": "^6.9.0",
    "babelify": "^7.3.0",
    "browser-sync": "^2.13.0",
    "browserify": "^13.0.1",
    "critical": "^0.7.3",
    "del": "^2.2.1",
    "glob": "^7.0.3",
    "gulp": "^3.9.1",
    "gulp-clean-css": "^2.0.11",
    "gulp-css-globbing": "^0.1.8",
    "gulp-eslint": "^3.0.1",
    "gulp-imagemin": "^3.0.3",
    "gulp-load-plugins": "^1.2.4",
    "gulp-newer": "^1.2.0",
    "gulp-plumber": "^1.1.0",
    "gulp-postcss": "^6.1.1",
    "gulp-prettify": "^0.4.0",
    "gulp-rev": "^7.1.0",
    "gulp-sass": "^2.3.1",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-twig-up-to-date": "^0.6.1",
    "gulp-uglify": "^1.5.4",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^3.7.0"
  },
  "dependencies": {
    "iterall": "^1.0.2",
    "normalize-scss": "^5.0.3",
    "smooth-scroll": "github:cferdinandi/smooth-scroll"
  }
}
